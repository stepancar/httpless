{
  "name": "@httpless/httpless",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/node": "^18.11.18",
    "lerna": "^3.22.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.9.10"
  },
  "scripts": {
    "build": "run-p clean run-s build:api",
    "build:api": "tsc -b ./packages/tsconfig.api-base.json",
    "bootstrap": "lerna bootstrap",
    "clean": "lerna run --parallel clean",
    "release": "lerna publish"
  }
}
